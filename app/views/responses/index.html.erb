<table>
  <thead>
    <tr>
      <th><%= t('name') %></th>
      <th><%= t('body') %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% responses.each do |response| %>
      <tr>
        <td><%= response.name %></td>
        <td><%= response.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>
